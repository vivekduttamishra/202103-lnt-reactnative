{
  "expo": {
    "name": "Books World",
    "slug": "books-world",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/book-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
        
        
      },
      "package": "in.conceptarchitect.booksworld"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
